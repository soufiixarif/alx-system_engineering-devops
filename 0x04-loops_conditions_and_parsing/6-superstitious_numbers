 #!/usr/bin/env bash
#Write a Bash script that displays numbers from 1 to 20 and:.

C=1
while [ $C -lt 21 ]
do
    case $C in
    4)
        echo $C
        echo "bad luck from China"
        ;;
    9)
        echo $C
        echo "bad luck from Japan"
        ;;
    17)
        echo $C
        echo "bad luck from Italy"
        ;;
    *)
        echo $C
        ;;
    esac
    let C=C+1
done